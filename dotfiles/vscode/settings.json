      ".monaco-list-row .monaco-tl-row .monaco-icon-label-container .label-name": "font-family: 'JetBrains Mono', Menlo, monospace; letter-spacing: -0.3px; font-size: 11.5px;",
      ".custom-statusbar .monaco-workbench .part.statusbar .statusbar-item-label": "font-family: 'JetBrains Mono', sans-serif; letter-spacing: -0.2px; font-size: 10.5px !impo√∑rtant;",
      ".quick-input-filter .monaco-inputbox": "font-family: 'JetBrains Mono' !important;",
      "body .monaco-workbench": "--al-transparency-percent: 0%; --al-tab-height: 36px; --al-tab-y-offset: 6px; --al-command-palette-blur-amount: 4px; --al-tab-fontSize: 11.5px;"
    },
    "window.titleBarStyle": "native",
    "terminal.external.osxExec": "Iterm.app",
    "animations.Enabled": true,
    "animations.Install-Method": "Apc Customize UI++",
